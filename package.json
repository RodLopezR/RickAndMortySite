{
  "name": "ricky-and-morty-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "BUILD_PATH='./docs' REACT_APP_BASE_URL='RickAndMortySite/' react-scripts build",
    "test": "react-scripts test --ci --coverage --watchAll=false",
    "test:watch": "react-scripts test --coverage --watchAll=true",
    "eject": "react-scripts eject",
    "prettier": "./node_modules/.bin/prettier --write ."
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "dotenv": "^10.0.0",
    "node-sass": "^4.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-paginate": "^7.1.3",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-paginate": "^7.1.1",
    "@types/react-router-dom": "^5.1.8",
    "cypress": "^8.3.1",
    "history": "^5.0.1",
    "nock": "^13.1.3",
    "prettier": "^2.3.2",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "!<rootDir>/src/index.tsx",
      "!<rootDir>/src/reportWebVitals.ts",
      "!<rootDir>/src/react-app-env.d.ts",
      "!<rootDir>/src/__tests__/setupTests.ts",
      "!<rootDir>/src/types/**",
      "!<rootDir>/src/**/*.snap"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70
      }
    }
  }
}
